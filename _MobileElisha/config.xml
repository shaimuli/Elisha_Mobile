<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.cordova.JetCopy" version="1.0.2" xmlns="http://www.w3.org/ns/widgets">
  <name>elisha</name>
  <description>elisha</description>
  <allow-navigation href="http://delek-elisha.co.il/*" />
  <author href="http://cordova.io" email="aaa@aaa.apache.org">elisha</author>
  <content src="index.html" />
  <access origin="*" />
  <icon src="icon.png" />
  <icon src="res/icon/android/icon-36-ldpi.png" />
  <icon src="res/icon/android/icon-48-mdpi.png" />
  <icon src="res/icon/android/icon-72-hdpi.png"/>
  <icon src="res/icon/android/icon-96-xhdpi.png" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.0" />
  <preference name="windows-phone-target-version" value="8.1" />
  <preference name="android-minSdkVersion" value="7" />

  <preference name="fullscreen" value="false" />
  <preference name="webviewbounce" value="false" />
  <preference name="useBrowserHistory" value="true" />
  <preference name="exit-on-suspend" value="false" />
  <preference name="permissions" value="none" />
  <preference name="target-device" value="universal" />
  <preference name="prerendered-icon" value="true" />
  <preference name="stay-in-webview" value="false" />
  <preference name="detect-data-types" value="true" />
  <preference name="show-splash-screen-spinner" value="true" />
  <preference name="auto-hide-splash-screen" value="true" />
  <preference name="disable-cursor" value="false" />
  <preference name="android-minSdkVersion" value="10" />
  <preference name="android-installLocation" value="auto" />
  <preference name="orientation" value="portrait"/>
  <preference name="AndroidPersistentFileLocation" value="Internal" />
  <preference name="phonegap-version" value="cli-6.5.0" />
  <vs:platformSpecificValues />

  <plugin name="cordova-plugin-device" />
  <plugin name="cordova-plugin-file" />
  <plugin name="cordova-plugin-statusbar" />

</widget>